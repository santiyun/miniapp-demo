  </cover-view>
  <cover-view class="" style="position: absolute; color: #f00; top: 10px; left: 10px; font-size: 28rpx; right: 10px">
    pusher-{{name}}--({{url !== '' ? cid : 'NO-Pushing'}})
  </cover-view>
