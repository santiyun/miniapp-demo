<view class="pusher-container" id="rtcpusher" style="top: {{y}}px; left: {{x}}px; width: {{width}}px; height: {{height}}px; position: absolute;">
  <live-pusher wx:if="{{url !== ''}}" style="height: {{height}}px; position: relative; width: 100%; " url="{{url}}" mode="RTC" aspect="{{aspect}}" class="camera" bindstatechange="stateChange" bindnetstatus="netStatus" background-mute="true" muted="{{muted}}" beauty="{{beauty}}"
    max-bitrate="500" min-bitrate="200" debug="{{debug}}" autopush />
